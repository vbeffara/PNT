abbrev W21 : Type _ := W1 2 ‚ÑÇ

namespace W21

variable {f : W21}

noncomputable def norm (f : ‚Ñù ‚Üí ‚ÑÇ) : ‚Ñù :=
    (‚à´ v, ‚Äñf v‚Äñ) + (4 * œÄ ^ 2)‚Åª¬π * (‚à´ v, ‚Äñderiv (deriv f) v‚Äñ)

