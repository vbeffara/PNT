instance : SMul (CS n ‚Ñù) (W1 n E) where smul := smul

noncomputable def L1_norm (f : ‚Ñù ‚Üí E) : ‚Ñù := ‚à´ v, ‚Äñf v‚Äñ

lemma L1_norm_add {f g : ‚Ñù ‚Üí E} (hf : Integrable f) (hg : Integrable g) :
    L1_norm (f + g) ‚â§ L1_norm f + L1_norm g := by
