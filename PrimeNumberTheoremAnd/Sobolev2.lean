lemma norm_mul0 (g : CS n ‚Ñù) (f : W1 n E) : norm1 (g ‚Ä¢ f) ‚â§ ‚Äñg‚Äñ * norm1 f := by
  convert_to ‚à´ v, ‚Äñg v ‚Ä¢ f v‚Äñ ‚â§ ‚Äñg‚Äñ * (‚à´ v, ‚Äñf v‚Äñ) using 0
  rw [‚Üê integral_mul_left]
